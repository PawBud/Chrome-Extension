{
    "name": "temp ext", 
    "description": "my ext",
    "version": "0.1.0",
    "manifest_version": 2,
    "icons": {
        "16": "icon1.png",
        "32": "icon2.png",
        "48": "icon3.png",
        "128": "icon4.png"
    },
    "background": {
        "scripts": ["./background.js"]
    },
    "options_page": "./options.html",
    "browser_action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "tab",
        "https://www.google.com/*",
        "storage"
    ]   
}